[user]
	email = oscar.baracos@gmail.com
	name = Oscar Baracos
[push]
	default = simple
[alias]
	st = status
	cm = commit -m
	psup = push --set-upstream origin
	cb = checkout -b
	pom = push origin master
	co = checkout
	lg = log --graph --pretty=format:'%Cred%h%Creset%d -%Cblue %s%Creset %Cgreen(%ar) %Cblue<%an>%Creset' --abbrev-commit --date=relative
	pr = pull --rebase
